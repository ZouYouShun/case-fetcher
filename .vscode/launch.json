  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["create", "${input:repoName}"]
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "repoName",
      "description": "Which repo you want to run test?",
      "options": [
        "1002",
      ],
      "default": "1000"
    }
  ]
}
